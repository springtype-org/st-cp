    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "author": "Aron Homberg, Michael Mannseicher",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/springtype-org/springtype/issues"
    },
    "homepage": "https://springtype.org",
    "dependencies": {
        "chalk": "^2.4.2",
        "cross-spawn": "^6.0.5"
    },
    "devDependencies": {
        "@types/node": "^12.11.7",
        "node": "^12.12.0",
        "typescript": "^3.3.4000"
    }
