{
    "name": "st-cp",
    "version": "2.0.0-alpha.2",
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "copy",
        "files",
        "recursive",
        "st-cp"
    ],
    "description": "CLI tool for platform independent, recursive copying of files and folders.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/springtype-org/springtype.git"
    },
    "scripts": {
        "prebuild": "npx st-rm-rf dist",
        "build": "tsc",
        "postbuild": "npx st-cp README.md package.json node_modules dist"
    },
    "engines": {
        "node": ">=10"
    },
    "bundledDependencies": [
        "chalk"
    ],
    "bin": {
        "st-cp": "./cli.js"
    },
    "author": "Aron Homberg, Michael Mannseicher",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/springtype-org/springtype/issues"
    },
    "homepage": "https://springtype.org",
    "dependencies": {
        "chalk": "^2.4.2",
        "cross-spawn": "^6.0.5"
    },
    "devDependencies": {
        "@types/node": "^12.11.7",
        "node": "^12.13.0",
        "typescript": "^3.6.4",
        "st-run": "2.0.0-alpha.10"
    }
}